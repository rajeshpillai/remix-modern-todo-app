{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "npm-run-all dev:build --parallel \"watch:*\"",
    "dev:build": "remix build",
    "watch:remix": "remix watch",
    "xwatch:pm2": "pm2-dev ./server.mjs",
    "watch:nm": "nodemon ./server.mjs",
    "prod:build": "remix build",
    "prod:start": "NODE_ENV=production node ./server.mjs",
    "typecheck": "tsc",
    "lint": "eslint \"app/*/*.{js,jsx}\" \"services/**/*.{js,jsx}\" \"controller/**/*.{js,jsx}\" \"instrumenter/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"app/**/*.{js,jsx}\" --fix",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mcansh/remix-fastify": "^2.7.4",
    "@prisma/client": "^4.15.0",
    "@remix-run/css-bundle": "^1.17.0",
    "@remix-run/node": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "bcrypt": "^5.1.0",
    "fastify": "^4.17.0",
    "isbot": "^3.6.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.17.0",
    "@remix-run/eslint-config": "^1.17.0",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "dotenv": "^16.3.1",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "patch-package": "^7.0.0",
    "pm2": "^5.3.0",
    "prisma": "^4.15.0",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "engines": {
    "node": ">=14"
  }
}
