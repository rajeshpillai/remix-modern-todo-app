{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "devx": "remix dev --manual -c \"node --watch-path server.js --watch server.js\"",
    "dev": "npm-run-all dev:build --parallel \"watch:*\"",
    "dev:build": "remix build",
    "watch:remix": "remix watch",
    "watch:nm": "nodemon ./server.mjs",
    "prod:build": "remix build",
    "prod:start": "NODE_ENV=production node ./server.mjs",
    "typecheck": "tsc",
    "lint": "eslint \"app/*/*.{js,jsx}\" \"services/**/*.{js,jsx}\" \"controller/**/*.{js,jsx}\" \"instrumenter/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"app/**/*.{js,jsx}\" --fix",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@remix-run/css-bundle": "^2.10.3",
    "@remix-run/express": "^2.10.3",
    "@remix-run/node": "^2.10.3",
    "@remix-run/react": "^2.10.3",
    "@tailwindcss/typography": "^0.5.9",
    "compression": "^1.7.4",
    "daisyui": "^3.6.4",
    "express": "^4.19.2",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.10.3",
    "@remix-run/eslint-config": "^2.10.3",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "dotenv": "^16.3.1",
    "eslint": "^8.38.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^7.0.0",
    "pm2": "^4.2.3",
    "prisma": "^4.15.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "engines": {
    "node": ">=14"
  }
}
